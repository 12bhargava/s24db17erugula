extends layout
 
block content
  h1= title
  - let results = [{Racquet_type:'Ball',Racquet_usedfor:'Tennis',Racquet_cost:10},{Racquet_type:'Shuttle',Racquet_usedfor:'badminton',Racquet_cost:20},{Racquet_type:'shoes',Racquet_usedfor:'walking',Racquet_cost:100}]
  div(class="container-fluid")
    div(class="row")
      each item, index in results  
        div(class="col-sm-2",onclick=`buy(${index})`)
          p #{item.Racquet_type}
          p #{item.Racquet_usedfor}
          p #{item.Racquet_cost}
  script.
    function buy(index) {
    alert("You purchased item " + index);}